{
     "manifest_version": 3,
  "name": "Remember Me",
  "description": "Save a personal reminder for each website.",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icons/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
   "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}